<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.itcast.mapper.LoginMapper">
    <select id="queryByUserId" parameterType="String" resultType="Admin">
        select * from admin where userId=#{userId};
    </select>
</mapper>